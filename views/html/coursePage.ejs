<body class="animsition">
<div class="page-wrapper">
    <!-- Include Dashboard Side Navigation Bar -->
    <%- include("../partials/dashboard_side_nav.ejs") %>

    <div class="page-container">
        <!-- Include Dashboard Header -->
        <%- include("../partials/dashboard_header.ejs") %>
        <!-- MAIN CONTENT-->
        <div class="main-content">
            <div class="section__content section__content--p30">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="overview-wrap">
                                <h2 class="title-1"><%= course %></h2>
                            </div>
                        </div>
                    </div>
                    <% let x = true; %>
                    <% filter.forEach(child => { %>
                        <% if (child.key !== "image") { %>
                            <% if (x) { %>
                                <button class="tablink" onclick="openPage('<%= child.key %>', this, 'red')" id="defaultOpen"><%= child.key %></button>
                                <% x = false; %>
                            <% } else { %>
                                <button class="tablink" onclick="openPage('<%= child.key %>', this, 'red')"><%= child.key %></button>
                            <% } %>
                            <div id="<%= child.key %>" class="tabcontent">
                                <% child.forEach(resource => { %>
                                    <a href="/course/<%= course %>/<%= child.key %><%= resource.child("name").val() %>"><h3><%= resource.child("name").val() %></h3></a>
                                    <br>
                                <% }) %>
                            </div>
                        <% } %>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>
</div>

