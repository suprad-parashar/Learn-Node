<body class="animsition">
<div class="page-wrapper">
    <!-- Include Dashboard Side Navigation Bar -->
    <%- include("../partials/dashboard_side_nav.ejs") %>

    <div class="page-container">
        <!-- Include Dashboard Header -->
        <%- include("../partials/dashboard_header.ejs") %>
        <div class="main-content">
            <div class="section__content section__content--p30">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="overview-wrap">
                                <h2 class="title-1"><%= branchName %></h2>
                            </div>
                        </div>
                    </div>
                    <% let x = true; %>
                    <% filter.forEach(child => { %>
                        <% if (child.key !== "image") { %>
                            <% if (x) { %>
                                <button class="tablink" onclick="openPage('<%= child.key %>', this, '#8C54A1')"
                                        id="defaultOpen"><%= child.key %></button>
                                <% x = false; %>
                            <% } else { %>
                                <button class="tablink"
                                        onclick="openPage('<%= child.key %>', this, '#8C54A1')"><%= child.key %></button>
                            <% } %>
                            <div id="<%= child.key %>" class="tabcontent">
                                <% let courses = []; %>
                                <% child.forEach(course => { %>
                                    <% courses.push(course.val()) %>
                                <% }) %>
                                <% courses.sort(); %>
                                <% courses.forEach(course => { %>
                                    <a href="/course/<%= course %>"><h3><%= course %></h3></a>
                                    <br>
                                <% }) %>
                            </div>
                        <% } %>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>
</div>

